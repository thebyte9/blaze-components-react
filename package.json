{
  "name": "blaze-react",
  "version": "0.0.1",
  "description": "React Component Framework for Blaze-CMS",
  "scripts": {
    "setup": "yarn install && yarn run build",
    "dev": "start-storybook",
    "clean": "lerna clean && yarn cache clean",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "build": "yarn workspaces foreach run build",
    "build-storybook": "NODE_ENV=production build-storybook -o ./storybook-static",
    "serve-storybook": "npx http-server ./storybook-static",
    "test": "node --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage --testPathPattern=/__tests__/ --passWithNoTests",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "pre-publish": "yarn build && yarn lint && yarn test",
    "analyse": "ANALYSE=true yarn workspaces foreach run build",
    "prerelease": "yarn build && yarn test && yarn storybook && GH_TOKEN=$GH_TOKEN_BLAZE yarn prerelease:version && yarn prerelease:publish",
    "prerelease:version": "npx lerna version --conventional-commits --conventional-prerelease --no-commit-hooks --create-release github",
    "prerelease:publish": "npx lerna publish from-git --dist-tag alpha",
    "release:version": "npx lerna version --conventional-commits --conventional-graduate --no-commit-hooks --create-release github",
    "release:publish": "npx lerna publish from-git",
    "postinstall": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebyte9/blaze-components-react.git"
  },
  "workspaces": [
    "packages/*"
  ],
